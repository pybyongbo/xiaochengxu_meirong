/**index.wxss**/

.page {
    display: flex;
    flex-direction: column;
    padding-top: 10px;
}

.section {
    width: 90%;
    margin: 0 auto 15px;
    position: relative;
    overflow: hidden;
}

.section .label {
    font-size: 14px;
    color: #666;
    margin-bottom: 10rpx;
}

.section .phcolor {
    color: #dedede;
}

.section .phcolor:last-child {
    padding-left: 0!important;
}

.section .text {
    border: 1px solid #dedede;
    padding-left: 10px;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #000;
}

.section .wxtxt55 {
    width: 55%;
    float: left;
}

.section .wxtxt100 {
    width: 96%;
    float: left;
}

.ar_picker {
    width: 100%;
    font-size: 30rpx;
    color: #000;
    background: #efeff4;
    margin: 0;
}

.ar_picker .ar_picker_wrap {
    width: 670rpx;
    background: #fff url(../../images/arrow.png) no-repeat right center;
    background-size: 25rpx 14rpx;
    background-position: 650rpx;
    border-radius: 20rpx;
    padding: 20rpx;
    margin: 20rpx auto;
}

.ar_cont {
    padding: 20rpx 60rpx;
    font-size: 30rpx;
    background: #fff;
    margin-bottom: 40rpx;
    line-height: 45rpx;
    color: #666666;
}

.ar_cont .ar_cont_h1 {
    font-size: 32rpx;
    font-weight: 600;
    line-height: 64rpx;
    color: #000;
}

.ar_date_time {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin: 30rpx 0rpx 60rpx;
}

.ar_date_time .ar_date,
.ar_date_time .ar_time {
    width: 47%;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.ar_date_time .ar_date image {
    width: 34rpx;
    height: 38rpx;
    margin-top: 25rpx;
}

.ar_date_time .ar_time image {
    width: 45rpx;
    height: 38rpx;
    margin-top: 25rpx;
}

.ar_date_time picker {
    width: 220rpx;
    border: 1px solid #ddd;
    border-radius: 8rpx;
    padding: 0rpx 10rpx;
    margin-left: 10rpx;
}

.section .disabled {
    background: #f5f7fa url(data:image/png;base64,) no-repeat right center;
    background-size: contain;
}

.section .textarea {
    width: 92%;
    height: 160rpx;
    margin: 10rpx auto;
    padding: 10rpx 25rpx;
}

button.btn {
    width: 90%;
    margin: 0 auto;
}

button.getcode {
    width: 35%!important;
    margin-left: 5%;
    float: left;
}


/*mask*/

.drawer_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10000;
    background: #000;
    opacity: 0.2;
    overflow: hidden;
}


/*content*/

.drawer_attr_box {
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 100001;
    background: #fff;
}

.drawer_content {
    padding: 20rpx 5%;
    height: 250rpx;
    box-sizing: content-box;
}

.drawer_title {
    float: left;
    width: 23%;
    height: 45rpx;
    line-height: 45rpx;
    padding: 20rpx;
    font: 32rpx "microsoft yahei";
    text-align: center;
    color: #666;
    margin-top: 20rpx;
    margin-left: 20rpx;
    margin-bottom: 18rpx;
}

.line {
    border: 1px solid #f8f8f8;
}

.active {
    background: url(data:image/png;base64,) no-repeat right top;
    background-size: 18% 60%;
    color: #bb473b;
}

.line {
    border: 1px solid #f8f8f8;
}